   "source": [
    "# Import the pandas library for data manipulation\n",
    "import pandas as pd\n",
    "# Import the numpy library for numerical operations\n",
    "import numpy as np\n",
    "# Import the IterativeImputer from scikit-learn for MICE imputation\n",
    "from sklearn.impute import IterativeImputer\n",
    "# Import the os library for operating system dependent functionality\n",
    "import os\n",
    "\n",
    "# Define the path to the folder containing the processed data\n",
    "processed_folder_path = os.path.join('data', '종관기상관측(ASOS)기상데이터', '가공후')\n",
    "\n",
    "# Define the path for the input file that needs to be processed\n",
    "file_to_process = os.path.join(processed_folder_path, 'ASOS_통합(1301~2412)_fillna.csv')\n",
    "# Define the path for the output file where the imputed data will be saved\n",
    "output_file = os.path.join(processed_folder_path, 'ASOS_통합(1301~2412)_fillna_mice.csv')\n",
    "\n",
    "# Create a list of column names that require imputation\n",
    "precipitation_columns = [\n",
    "    '강수 계속시간(hr)',\n",
    "    '10분 최다 강수량(mm)',\n",
    "    '10분 최다강수량 시각(hhmi)',\n",
    "    '1시간 최다강수량(mm)',\n",
    "    '1시간 최다 강수량 시각(hhmi)',\n",
    "    '일강수량(mm)'\n",
    "]\n",
    "\n",
    "# Create an instance of the IterativeImputer, which will be used for MICE\n",
    "# Set a random_state for reproducibility of the results\n",
    "mice_imputer = IterativeImputer(random_state=0)\n",
    "\n",
    "# Define the number of rows per chunk for processing large files\n",
    "chunk_size = 10000\n",
    "# Create an empty list to store the processed chunks of data\n",
    "processed_chunks = []\n",
    "\n",
    "# Print a message indicating the start of the imputation process\n",
    "print(f\"Starting MICE imputation for {file_to_process}...\")\n",
    "\n",
    "# Loop through the input CSV file, reading it in chunks\n",
    "for chunk in pd.read_csv(file_to_process, chunksize=chunk_size, encoding='cp949', low_memory=False):\n",
    "    # Create a copy of the chunk to avoid SettingWithCopyWarning\n",
    "    chunk_copy = chunk.copy()\n",
    "    # Select the specific columns from the chunk that are to be imputed\n",
    "    impute_data = chunk_copy[precipitation_columns]\n",
    "\n",
    "    # Check if there are any missing (NaN) values in the selected data\n",
    "    if impute_data.isnull().values.any():\n",
    "        # If missing values exist, apply the MICE imputation\n",
    "        imputed_data = mice_imputer.fit_transform(impute_data)\n",
    "        # Update the columns in the copied chunk with the new, imputed data\n",
    "        chunk_copy[precipitation_columns] = imputed_data\n",
    "    \n",
    "    # Add the processed chunk (with imputed values) to the list of processed chunks\n",
    "    processed_chunks.append(chunk_copy)\n",
    "    # Print a status update for each processed chunk\n",
    "    print(f\"Processed a chunk of {len(chunk_copy)} rows...\")\n",
    "\n",
    "# Combine all the processed chunks back into a single DataFrame\n",
    "df_imputed = pd.concat(processed_chunks, ignore_index=True)\n",
    "\n",
    "# Save the final DataFrame with imputed data to a new CSV file\n",
    "df_imputed.to_csv(output_file, index=False, encoding='cp949')\n",
    "\n",
    "# Print a confirmation message that the process is complete and the file is saved\n",
    "print(f\"MICE imputation complete. Saved imputed data to {output_file}\")\n",
    "# Print a header for displaying the first few rows of the result\n",
    "print(\"Displaying the first 5 rows of the imputed data:\")\n",
    "# Display the first 5 rows of the imputed DataFrame to verify the result\n",
    "print(df_imputed.head())"
   ]
